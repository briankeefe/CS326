{
    "builds": [
        {
            "src": "*.html",
            "use": "@now/static"
        },
        {
            "src": "*.js",
            "use": "@now/node"
        },
        {
            "src": "/static/*.html",
            "use": "@now/static"
        },
        {
            "src": "/static/*.js",
            "use": "@now/node"
        }
    ]
}